clear;
clc
N = 10000;
ITR = 1000;

for k = 1 : ITR
a(1) = 1;
a(2) = 2;
t = cputime;        % tic
for i = 3 : N
    a(i) = 0.9*a(i - 1) - 0.3*a(i - 2);
end
dt1 = cputime - t   % toc

b = zeros(1,N);
b(1) = 1;
b(2) = 2;
t = cputime;        % tic
for i = 3 : N
    b(i) = 0.9*b(i - 1)-0.3*b(i - 2);
end
dt2 = cputime - t   % toc
dt1/dt2

norm(a - b)